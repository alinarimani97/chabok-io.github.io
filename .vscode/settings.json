{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "eslint.validate": [
        "javascript",
        "typescript",
        "vue"
    ],
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "editor.detectIndentation": false,
	"css.validate": false,
	"files.associations": {
		"*.json": "jsonc"
	}
}